[
  {
    "name": "vault",
    "image": "vault:0.7.3",
    "portMappings": [
      {
        "containerPort": 8200,
        "hostPort": 8200,
        "protocol": "tcp"
      }
    ],
    "memory": 128,
    "command": [
      "vault",
      "server",
      "-config=/vault/config"
    ],
    "environment": [

    ],
    "privileged": true,
    "mountPoints": [
      {
        "sourceVolume": "vault_config",
        "containerPath": "/vault/config",
        "readOnly": false
      },
      {
        "sourceVolume": "vault_logs",
        "containerPath": "/vault/logs",
        "readOnly": false
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "us-east-1",
        "awslogs-group": "${log_group}"
      }
    }
  }
]
